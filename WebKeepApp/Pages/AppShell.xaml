<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="WebKeepApp.Pages.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:WebKeepApp.Pages"
    Shell.FlyoutBehavior="Disabled"
    Title="WebKeepApp">

    <ShellContent
        Title="Login"
        ContentTemplate="{DataTemplate local:LoginPage}"
        Route="LoginPage"/>
    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage"/>
    <ShellContent
        Title="New Website"
        ContentTemplate="{DataTemplate local:NewWebsitePage}"
        Route="NewWebsitePage"/>

</Shell>
