<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="WebKeepApp.Pages.LoginPage">

        <StackLayout>
            <Label Text="Login" FontSize="Large" HorizontalOptions="Center" />
            <Entry Placeholder="Username" x:Name="UsernameEntry" Focused="UsernameEntry_Focused" />
            <Entry Placeholder="Password" IsPassword="True" x:Name="PasswordEntry" />
        </StackLayout>
    
</ContentPage>
